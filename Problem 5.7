# include <stdio.h>

int main ()
{
    int i;
    double sum1=0,sum2=0,sum3=0;
    
    for(i=1; i<=100; ++i)
    {
        sum1 += i;
        if(i<=50)
        {
            sum2 += i*i;
        }
        if(i<=10)
        {
            sum3 += 1.0/i;
        }
        
    }
    printf("sn=%lf\n",sum1+sum2+sum3);
    return 0;
}
