# include <stdio.h>

int main()
{
    double score;
    
    do
    {
        printf("输入分数:");
        scanf("%lf",&score);
        if(score<0 || score>100)
        {
            printf("请重新");
        }
        else
        {
            break;
        }
    }
    while(1);
    
    switch(int(score)/10)
    {
    case 10:
    case 9:
        printf("等级为A\n");
        break;
    case 8:
        printf("等级为B\n");
        break;
    case 7:
        printf("等级为C\n");
        break;
    case 6:
        printf("等级为D\n");
        break;
    default:
        printf("等级为E\n");
    }
    
    return 0;
}
