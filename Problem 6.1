# include <stdio.h>

int main()
{
    int a[100];
    int i,j,k;
    
    for(i=0; i<100; ++i)    //建立数组，将1到100的自然数写入
    {
        a[i]=i+1;
    }
    a[0]=0;                    //去掉自然数1
    for(i=1; i<100; ++i)    //最外层从2到99循环
    {
        if(a[i]!=0)            //从2开始第一个没被去掉的数是素数
        {
            printf("%d ",a[i]);        //把质数输出出来
            for(j=1; j<100; j++)    //嵌套循环，a[1]到a[99]中等于素数倍数的数去除（改为0）
            {
                for(k=2; k<=100/a[i]; ++k)
                {
                    if(a[j]==a[i]*k) 
                    {
                        a[j]=0;
                    }
                }
            }
        }
    }
    printf("\n");
    return 0;
}
