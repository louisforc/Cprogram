# include <stdio.h>
# define N 10

int main()
{
    int i,j;
    int a[N][N]={0};
    
    for(i=0; i<N; ++i)
    {
        for(j=0; j<N; ++j)
        {
            if(0==j)
            {
                a[i][j]=1;
            }
            else if(i>0)
            {
                a[i][j] = a[i-1][j-1] + a[i-1][j];
            }
            if(0!=a[i][j])
            {
                printf("%-4d",a[i][j]);
            }
        }
        printf("\n");
    }
    
    return 0;
}
