# include <stdio.h>
# define N 5

int main()
{
    int a[N]={1,3,5,7,9};
    int b[N+1];
    int i;
    
    printf("原来已排好序的数组为:\n");
    for(i=0; i<N; ++i)
    {
        printf("%d ",a[i]);
    }
    printf("\n");
    
    printf("输入要插入的数:\n");
    scanf("%d",&b[N]);
    
    for(i=0; i<N; ++i)
    {
        if(a[i]<=b[N])
        {
            b[i]=a[i];
        }
        else
        {
            b[i]=b[N];
            b[N]=a[i];
        }
    }
    
    printf("新的数组为:\n");
    for(i=0; i<=N; ++i)
    {
        printf("%d ",b[i]);
    }
    printf("\n");
    return 0;
}
